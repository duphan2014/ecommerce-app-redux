{
    "compilerOptions": {
      "jsx": "react",
      "allowJs": true,
      "checkJs": false,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }